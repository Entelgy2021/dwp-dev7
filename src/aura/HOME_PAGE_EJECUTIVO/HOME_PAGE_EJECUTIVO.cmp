<aura:component implements="lightning:HomeTemplate" description="Main column and right sidebar. On a phone, the regions are of equal width">
    <aura:attribute name="uleft" type="Aura.Component[]" />
    <aura:attribute name="uright" type="Aura.Component[]" />
    <aura:attribute name="left1" type="Aura.Component[]" />
    <aura:attribute name="right1" type="Aura.Component[]" />
    <aura:attribute name="main1" type="Aura.Component[]" /> 
    <aura:attribute name="left2" type="Aura.Component[]" />
    <aura:attribute name="right2" type="Aura.Component[]" />
    <aura:attribute name="main2" type="Aura.Component[]" /> 
    <aura:attribute name="left3" type="Aura.Component[]" />
    <aura:attribute name="right3" type="Aura.Component[]" />
    <aura:attribute name="main3" type="Aura.Component[]" /> 
      <aura:attribute name="left" type="Aura.Component[]" />
     <aura:attribute name="right" type="Aura.Component[]" />
     <aura:attribute name="main" type="Aura.Component[]" />
    <div>
        <lightning:layout>
            <div class="hl1">
            <lightning:layoutItem 
                                  class="slds-m-left_small slds-m-top_small">
                {!v.uleft}
            </lightning:layoutItem>
            </div>  
              <div class="hl2">
            <lightning:layoutItem 
                                  class="slds-m-left_small slds-m-top_small">
                {!v.uright}
            </lightning:layoutItem>
            </div>     
        </lightning:layout>
        <lightning:layout>
            <div class="l1" >
            <lightning:layoutItem
                                  class="slds-m-left_small slds-m-top_small">
                {!v.left1}
            </lightning:layoutItem>
            </div>   
            <div class="l2" >
            <lightning:layoutItem
                                  class="slds-m-left_small slds-m-top_small">
                {!v.main1}
            </lightning:layoutItem>
            </div>
             <div class="l3" >
            <lightning:layoutItem 
                                  class="slds-m-left_small slds-m-top_small">
                {!v.right1}
            </lightning:layoutItem>
            </div>    
        </lightning:layout>
        <p/>
        <lightning:layout>
            <div class="l1">
            <lightning:layoutItem
                                  class="slds-m-left_small slds-m-top_small">
                {!v.left2}
            </lightning:layoutItem>
            </div>   
            <div class="hl2" >
            <lightning:layoutItem
                                  class="slds-m-left_small slds-m-top_small">
                {!v.main2}
            </lightning:layoutItem>
            </div>
        </lightning:layout>
        <p/>
        <lightning:layout>
            <div class="l1" >
            <lightning:layoutItem
                                  class="slds-m-left_small slds-m-top_small">
                {!v.left3}
            </lightning:layoutItem>
            </div>   
            <div class="l2" >
            <lightning:layoutItem
                                  class="slds-m-left_small slds-m-top_small">
                {!v.main3}
            </lightning:layoutItem>
            </div>
            <div class="l3" >
            <lightning:layoutItem 
                                  class="slds-m-left_small slds-m-top_small">
                {!v.right3}
            </lightning:layoutItem>
            </div>    
        </lightning:layout>
    </div>    
</aura:component>