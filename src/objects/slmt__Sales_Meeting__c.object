    <validationRules>
        <fullName>Invalid_DateTime_Ejecutivo_Planning_Type</fullName>
        <active>true</active>
        <description>If SM is Planning type and colaborator is ejecutivo, and BE_DateTime__c is greater than the first 6 working days of the month</description>
        <errorConditionFormula>IF(ISPICKVAL(Type_of_meeting__c, &apos;Planning&apos;) &amp;&amp; 
	(Collaborator__r.prof_position_id__c == &apos;E03&apos; || 
		Collaborator__r.prof_position_id__c == &apos;I32&apos;),
	IF(DATEVALUE(DateTime__c) &gt;= DATE(YEAR(DATEVALUE(DateTime__c)), MONTH(DATEVALUE(DateTime__c)), 1) &amp;&amp;
		DATEVALUE(DateTime__c)&lt;= DATE(YEAR(DATEVALUE(DateTime__c)), MONTH(DATEVALUE(DateTime__c)), CASE(
																					MOD( DATE(YEAR(DATEVALUE(DateTime__c)), MONTH(DATEVALUE(DateTime__c)), 1) - DATE( 1900, 1, 8 ), 7 ),
																					0, 8,
																					1, 8,
																					2, 8,
																					3, 8,
																					4, 8,
																					5, 10,
																					9)), 
		false, true), 
	false)</errorConditionFormula>
        <errorMessage>No puede colocar una fecha mayor a los 6 pimeros días utiles del mes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_DateTime</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISCHANGED( DateTime__c ) , DateTime__c &lt;  NOW())</errorConditionFormula>
        <errorMessage>No se puede crear una reunión individual con fecha y hora pasada</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Finalized_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( NOW() &lt;  DateTime__c ,  ISPICKVAL( slmt__mngmt_plan_meeting_status_name__c , &apos;Finalized&apos;) )</errorConditionFormula>
        <errorMessage>No se puede finalizar una reunión individual antes de la fecha y hora programda.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
