    <validationRules>
        <fullName>BE_Check_Amount_Cancelled</fullName>
        <active>false</active>
        <description>El monto cancelado no puede ser mayor al monto original asociado al producto</description>
        <errorConditionFormula>AND(!ISBLANK(Opp_amount__c) ,Opp_amount__c &lt; amount_cancelled__c)</errorConditionFormula>
        <errorDisplayField>amount_cancelled__c</errorDisplayField>
        <errorMessage>El monto cancelado no puede ser mayor al monto original asociado al producto</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BE_ValidateReturnExtension</fullName>
        <active>true</active>
        <errorConditionFormula>AND( extension__c = true, return__c=true)</errorConditionFormula>
        <errorMessage>No se puede prorrogar y retomar un cancelación al mismo tiempo</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Calce_Detail_creation_handling</fullName>
        <active>false</active>
        <description>Controla que solo se pueda crear una oportunidad como resultado de cancelación parcialmente cancelada en el calce del día siguiente.</description>
        <errorConditionFormula>ISPICKVAL( Upsert_New_Calce_Detail__c , &apos;3&apos;)</errorConditionFormula>
        <errorDisplayField>amount_cancelled__c</errorDisplayField>
        <errorMessage>Ya se ha creado una oportunidad en el calce del día de mañana relacionada a esta cancelación parcial. Por favor, actualice el monto cancelado en dicho calce.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HasCalceDetails</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( IsNew() ,  HasChild__c = true )</errorConditionFormula>
        <errorMessage>error</errorMessage>
    </validationRules>
