        <fullName>acpl__gf_event_date_Q__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(
                    MONTH(acpl__gf_event_date__c) &lt; 4, &apos;1Q&apos; &amp; TEXT(YEAR(acpl__gf_event_date__c)),
                    IF(
                                MONTH(acpl__gf_event_date__c) &lt; 7, &apos;2Q&apos; &amp; TEXT(YEAR(acpl__gf_event_date__c)),
                                IF(
                                                MONTH(acpl__gf_event_date__c) &lt; 10, &apos;3Q&apos; &amp; TEXT(YEAR(acpl__gf_event_date__c)),
                                                &apos;4Q&apos; &amp; TEXT(YEAR(acpl__gf_event_date__c))
                                )
