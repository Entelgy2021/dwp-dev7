    <validationRules>
        <fullName>Assign_Client_Group</fullName>
        <active>true</active>
        <description>Validación para asignar un cliente a una acción comercial perteneciente a un Grupo Económico</description>
        <errorConditionFormula>acpl__gf_account_planning_id__r.acpl__participant_id__r.RecordType.DeveloperName == &apos;Record_Type_Economic_Group&apos; &amp;&amp;
acpl__gf_account_planning_id__r.acpl__participant_id__c != ClientAssigned__r.ParentId</errorConditionFormula>
        <errorDisplayField>ClientAssigned__c</errorDisplayField>
        <errorMessage>Debe asignar la variable a un cliente del grupo económico.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>acpl__Expected_completion_date_TODAY</fullName>
