    <validationRules>
        <fullName>LinkedAccount_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(
OR(
ISBLANK( cpliq_Cuenta_vinculada__c ),
REGEX( cpliq_Cuenta_vinculada__c , &quot;[0-9]{4}-[0-9]{4}-[0-9]{10}&quot;)
)
)</errorConditionFormula>
        <errorMessage>Linked Account format not valid, correct: 0000-0000-000000000</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidDueDate</fullName>
        <active>true</active>
        <errorConditionFormula>cpliq_Fecha_Vencimiento__c &lt;=Today()</errorConditionFormula>
        <errorMessage>Due Date must be major than today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidTerm</fullName>
        <active>true</active>
        <errorConditionFormula>IF(cpliq_n__c&lt;0 ,true,If( cpliq_n_gracia__c&lt;0 &amp;&amp;  IsPickVal(cpliq_Tipo_amortizacion__c,&apos;02&apos;),true,if( gipr_Plazo__c &lt;0,true,false)))</errorConditionFormula>
        <errorMessage>Please define a valid term, should be major than zero.</errorMessage>
    </validationRules>
