    <validationRules>
        <fullName>BE_CA_ChangeStatus</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(altm__alert_stage_type__c), 
OR($Profile.Name=&apos;Ejecutivo&apos;, $Profile.Name = &apos;Analista&apos;,
$Profile.Name = &apos;Especialista&apos;,$Profile.Name = &apos;Operativo&apos;,
$Profile.Name = &apos;Migracion&apos;),
RecordType.DeveloperName=&apos;BE_CommercialAlert&apos;,
OR(				
ISPICKVAL(PRIORVALUE(altm__alert_stage_type__c),&quot;Won&quot;),
ISPICKVAL(PRIORVALUE(altm__alert_stage_type__c),&quot;Discarded&quot;),
OR(
AND(
				ISPICKVAL(altm__alert_stage_type__c,&quot;Pending&quot;),
				ISPICKVAL(PRIORVALUE(altm__alert_stage_type__c),&quot;InManagement&quot;)
)
)
))</errorConditionFormula>
        <errorMessage>No se puede modificar el estado de la Alerta Comercial.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BE_ValidateComments</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
			OR($Profile.Name=&apos;Ejecutivo&apos;, $Profile.Name = &apos;Analista&apos;,
			$Profile.Name = &apos;Especialista&apos;,$Profile.Name = &apos;Operativo&apos;,
			$Profile.Name = &apos;Migracion&apos;),
   OR(ISPICKVAL(altm__commercial_alert_record_type__c, &apos;Management&apos;), 
			ISPICKVAL(altm__commercial_alert_record_type__c, &apos;Current&apos;)),
			ISPICKVAL(altm__alert_stage_type__c, &apos;Discarded&apos;),
   altm__Rejection_reason_desc__c	= null)</errorConditionFormula>
        <errorDisplayField>altm__Rejection_reason_desc__c</errorDisplayField>
        <errorMessage>Debe registrar un comentario.</errorMessage>
    </validationRules>
