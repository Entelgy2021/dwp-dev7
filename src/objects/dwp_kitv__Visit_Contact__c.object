    <validationRules>
        <fullName>Valid_visit_null</fullName>
        <active>true</active>
        <errorConditionFormula>if(ISNEW() ,IF( ISBLANK( dwp_kitv__contact_id__c ) ,true,false),false)</errorConditionFormula>
