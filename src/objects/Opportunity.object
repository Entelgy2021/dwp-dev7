    <sharingModel>Read</sharingModel>
    <validationRules>
        <fullName>Account_Required_Validation</fullName>
        <active>true</active>
        <description>Cannot be created Opportunity without an Account</description>
        <errorConditionFormula>AccountId == null</errorConditionFormula>
        <errorMessage>The account is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Validation_02_OnlyCommercial</fullName>
        <active>true</active>
        <description>Validate the creation in this Stage only for a Commercial Profile</description>
        <errorConditionFormula>(($Profile.Name!=&apos;Administrador del sistema&apos; &amp;&amp;$Profile.Name!=&apos;Migracion&apos;)&amp;&amp; IsNew())&amp;&amp; (!isPickVal(StageName,&apos;02&apos;) ||($Profile.Name!=&apos;Ejecutivo&apos;&amp;&amp;$Profile.Name!=&apos;Especialista&apos;))</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Only Commercial profile can create Opportunities for this stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Validation_ChangeOwner</fullName>
        <active>true</active>
        <errorConditionFormula>(ISCHANGED(OwnerId) &amp;&amp; $Profile.Name!=&apos;Administrador del sistema&apos;) &amp;&amp; ((Owner.Profile.Name=&apos;Operativo&apos; || Owner.Profile.Name=&apos;Gerente&apos; || Owner.Profile.Name=&apos;Analista&apos; ||
Owner.Profile.Name=&apos;Asistente&apos;) || (!ISPICKVAL(StageName, &apos;01&apos;) &amp;&amp; !ISPICKVAL(StageName,&apos;02&apos;) &amp;&amp; !ISPICKVAL(StageName ,&apos;03&apos;)))</errorConditionFormula>
        <errorMessage>Cannot change the owner of this opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Validation_ChangeStage</fullName>
        <active>true</active>
        <description>Please use the buttons to progress the opportunity.</description>
        <errorConditionFormula>AND( 
NOT(isProcess__c), 
$Profile.Name!=&apos;Administrador del sistema&apos;, 
$Profile.Name!=&apos;System Administrator&apos;, 
$Profile.Name!=&apos;Migracion&apos;, 
OR(ISCHANGED(StageName),ISCHANGED(opportunity_status_type__c)), 
OR( OR(TEXT(StageName) = &apos;04&apos;, TEXT(StageName) = &apos;05&apos;, TEXT(StageName) = &apos;06&apos;, TEXT(StageName) = &apos;07&apos; ), 
    AND( OR(TEXT(StageName) = &apos;04&apos;, TEXT(StageName) = &apos;05&apos;, TEXT(StageName) = &apos;06&apos;, TEXT(StageName) = &apos;07&apos; ), OR(TEXT(PRIORVALUE(StageName)) = &apos;01&apos;, TEXT(PRIORVALUE(StageName)) = &apos;02&apos;, TEXT(PRIORVALUE(StageName)) = &apos;03&apos;) ),
	AND( OR(TEXT(StageName) = &apos;01&apos;, TEXT(StageName) = &apos;02&apos;, TEXT(StageName) = &apos;03&apos; ), OR(TEXT(PRIORVALUE(StageName)) = &apos;04&apos;, TEXT(PRIORVALUE(StageName)) = &apos;05&apos;, TEXT(PRIORVALUE(StageName)) = &apos;06&apos;, TEXT(PRIORVALUE(StageName)) = &apos;07&apos;) )
  ) 
)</errorConditionFormula>
        <errorMessage>Please use the buttons to progress the opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Validation_Commercial_Action</fullName>
        <active>false</active>
        <description>you can not create an opportunity with  stage other than Comercial Actions</description>
        <errorConditionFormula>AND(
            IsNew(),
            OR(
            $Profile.Name == 'Administrador del sistema',
            $Profile.Name == 'Migracion')
            
            )</errorConditionFormula>
        <errorMessage>you can not create an opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount_Negative_Value</fullName>
        <active>true</active>
        <description>verify if amount on opportunity is negative</description>
        <errorConditionFormula>Amount &lt; 0</errorConditionFormula>
        <errorMessage>The amount must be greater than zero</errorMessage>
    </validationRules>
	<validationRules>
        <fullName>Analist_OppCloseDate_Edition</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND($Profile.Name == &apos;Analista&apos;, ISCHANGED( CloseDate )), TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>You don&apos;t have permission to modify the close date of the opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Analist_OppName_Edition</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND($Profile.Name == &apos;Analista&apos;, ISCHANGED( Name )), TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You don&apos;t have permission to modify the name of the opportunity.</errorMessage>
    </validationRules>
</CustomObject>
