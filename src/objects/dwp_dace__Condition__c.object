        <errorDisplayField>dwp_dace__operator_type__c</errorDisplayField>
        <errorMessage>Please, select an operator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dwp_dace__ValueFormatWhenCheckboxFieldType</fullName>
        <active>true</active>
        <description>Check if the value has the correct format when the field type is Checkbox</description>
        <errorConditionFormula>IF(AND(TEXT( dwp_dace__field_type__c )== &apos;Checkbox&apos; , UPPER(dwp_dace__field_value_1__c) &lt;&gt; UPPER(&apos;TRUE&apos;),UPPER(dwp_dace__field_value_1__c) &lt;&gt; UPPER(&apos;FALSE&apos;)),true,false)</errorConditionFormula>
        <errorDisplayField>dwp_dace__field_value_1__c</errorDisplayField>
        <errorMessage>Please, set the value to true or false</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dwp_dace__ValueFormatWhenFieldTypeNumber</fullName>
        <active>true</active>
        <description>Check the value format when the field type is Number, Percent or Currency</description>
        <errorConditionFormula>IF(AND(AND(TEXT( dwp_dace__operator_type__c ) &lt;&gt; &apos;ID_EMPTY&apos;,TEXT( dwp_dace__operator_type__c ) &lt;&gt; &apos;ID_NOTEMPTY&apos;),OR(TEXT( dwp_dace__field_type__c ) == &apos;Number&apos;,TEXT( dwp_dace__field_type__c ) == &apos;Percent&apos;,TEXT( dwp_dace__field_type__c ) == &apos;Currency&apos;),NOT(ISNUMBER( dwp_dace__field_value_1__c ))),true,false)</errorConditionFormula>
        <errorDisplayField>dwp_dace__field_value_1__c</errorDisplayField>
        <errorMessage>Enter a valid number format</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dwp_dace__ValueRequired</fullName>
        <active>true</active>
        <description>Validates if the value has been specified when the operator has specific values</description>
        <errorConditionFormula>IF(AND(AND(TEXT(dwp_dace__operator_type__c) &lt;&gt;  &apos;ID_EMPTY&apos; ,TEXT(dwp_dace__operator_type__c) &lt;&gt; &apos;ID_NOTEMPTY&apos;,NOT(ISBLANK(TEXT(dwp_dace__operator_type__c))) ),OR(ISBLANK( dwp_dace__field_value_1__c ),ISNULL(dwp_dace__field_value_1__c ))),true,false)</errorConditionFormula>
        <errorDisplayField>dwp_dace__field_value_1__c</errorDisplayField>
        <errorMessage>Please, specify a value</errorMessage>
    </validationRules>
