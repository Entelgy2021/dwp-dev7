    <validationRules>
        <fullName>dwp_dace__FieldTypeRequired</fullName>
        <active>true</active>
        <description>Validates if the field type has been specified when a field has been informed</description>
        <errorConditionFormula>IF(AND(NOT(ISBLANK( dwp_dace__field_api_name__c )),ISBLANK(TEXT(dwp_dace__field_type__c) )),true,false)</errorConditionFormula>
        <errorDisplayField>dwp_dace__field_type__c</errorDisplayField>
        <errorMessage>Please, select a field type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dwp_dace__GroupBehaviourNotAllowedIfFieldSpecified</fullName>
        <active>true</active>
        <description>The group behaviour should not be defined if the a field has been specified</description>
        <errorConditionFormula>IF(AND(NOT(ISBLANK(dwp_dace__field_api_name__c )),NOT(ISBLANK( TEXT(dwp_dace__group_behaviour_type__c) ))),true,false)</errorConditionFormula>
        <errorDisplayField>dwp_dace__group_behaviour_type__c</errorDisplayField>
        <errorMessage>Please, clear the group behaviour is a field has been specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dwp_dace__GroupBehaviourReqIfFieldNotSpecified</fullName>
        <active>true</active>
        <description>Group behaviour is required if field has not been specified</description>
        <errorConditionFormula>IF(AND(ISBLANK( dwp_dace__field_api_name__c ),ISBLANK(TEXT( dwp_dace__group_behaviour_type__c ))),true,false)</errorConditionFormula>
        <errorDisplayField>dwp_dace__group_behaviour_type__c</errorDisplayField>
        <errorMessage>Please, specified a group behaviour</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dwp_dace__OpertorRequired</fullName>
