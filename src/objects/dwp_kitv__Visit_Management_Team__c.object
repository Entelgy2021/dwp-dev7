    <validationRules>
        <fullName>User_is_empty</fullName>
        <active>false</active>
        <errorConditionFormula>if(ISNEW() ,IF( ISBLANK(  dwp_kitv__user_id__c ) ,true,false),false)</errorConditionFormula>
        <errorDisplayField>dwp_kitv__user_id__c</errorDisplayField>
        <errorMessage>The User is empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Visit_is_Active</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(dwp_kitv__visit_id__r.dwp_kitv__visit_status_type__c, &apos;06&apos;),
$Profile.Name != $Label.profAdministrator,
$Profile.Name != &quot;Migracion&quot;)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Not is possible create the Visit Management Team, the visit is in status ended</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dwp_kitv__Change_user</fullName>
        <active>true</active>
        <description>You cannot change the user.</description>
        <errorConditionFormula>ISCHANGED(  dwp_kitv__user_id__c )</errorConditionFormula>
        <errorMessage>Is not allowed change the user. Please, delete the visit management team.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dwp_kitv__User_owner</fullName>
        <active>true</active>
        <description>The user cannot be the same that visit owner.</description>
        <errorConditionFormula>AND(
$Setup.dwp_kitv__Visit_validations_acc_cs__c.dwp_kitv__User_Team_Member__c == false, dwp_kitv__user_id__c  =  dwp_kitv__visit_id__r.OwnerId)</errorConditionFormula>
        <errorDisplayField>dwp_kitv__user_id__c</errorDisplayField>
        <errorMessage>You cannot insert the Owner of the visit.</errorMessage>
    </validationRules>
