        <errorConditionFormula>AND (ISCHANGED(status__c),ISPICKVAL(PRIORVALUE(status__c),&apos;Compartido&apos;),manager_Id__c&lt;&gt;$User.Id,$Profile.Name &lt;&gt; &apos;System Administrator&apos;,$Profile.Name &lt;&gt; &apos;Administrador del sistema&apos;)</errorConditionFormula>
